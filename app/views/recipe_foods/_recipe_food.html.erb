<div id="<%= dom_id recipe_food %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Quantity:</strong>
    <%= recipe_food.quantity %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Recipe:</strong>
    <%= recipe_food.recipe_id %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Food:</strong>
    <%= recipe_food.food_id %>
  </p>
</div>
